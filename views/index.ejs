<!DOCTYPE html>
<html>
  <head>
    <title>StaticInfo</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

    <% include templates/header.ejs%>

      <p><%= name%></p>

      <h3><%= videos.categoryName%></h3>

      <ul>
          <% videos.categories.forEach(function (item) { %>

                <li><a href='/cat=<%=item.categoryID%>'><%=item.categoryName%></a></li>
           <% }); %>



      </ul>


  </body>
</html>
